{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",
  "Routes": [
    {
      "DownstreamPathTemplate": "/{everything}",
      "DownstreamScheme": "http",
      "DownstreamHostAndPorts": [
        {
          "Host": "localhost",
          "Port": 1214
        }
      ],
      "UpstreamPathTemplate": "/api-auth/{everything}",
      "UpstreamHttpMethod": [
        "POST",
        "PUT",
        "GET",
        "DELETE"
      ]
    }
    
  ]
}
